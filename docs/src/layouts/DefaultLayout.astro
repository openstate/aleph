---
import { DocsLayout } from 'astro-theme-docs/components';
import options from '../options.json';
---

<DocsLayout
  options={{ ...options, sidebar: { sections: [] } }}
  {...Astro.props}
>
  <slot />
  <slot name="sidebar" slot="sidebar" />
  <slot name="meta" slot="meta" />
  <slot name="toc" slot="toc" />
</DocsLayout>
